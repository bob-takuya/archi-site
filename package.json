{
  "name": "archi-site",
  "version": "1.1.0",
  "description": "日本の建築マップ - 日本の建築作品データベースとマップ表示アプリケーション",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "npx playwright test",
    "test:e2e:headed": "npx playwright test --headed",
    "test:e2e:debug": "npx playwright test --debug",
    "test:e2e:chromium": "npx playwright test --project=chromium",
    "test:e2e:firefox": "npx playwright test --project=firefox",
    "test:e2e:webkit": "npx playwright test --project=webkit",
    "test:e2e:mobile": "npx playwright test --project='Mobile Chrome' --project='Mobile Safari'",
    "test:e2e:performance": "npx playwright test tests/e2e/performance.spec.ts",
    "test:e2e:responsive": "npx playwright test tests/e2e/responsive-design.spec.ts",
    "test:e2e:map": "npx playwright test tests/e2e/map-functionality.spec.ts",
    "test:ui": "npx playwright test --ui",
    "test:report": "npx playwright show-report",
    "test:full": "npm run lint && npm run type-check && npm run test && npm run test:e2e",
    "test:quality-gate": "npm run test:full && node scripts/quality-gate-check.js",
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "prepare-db": "node scripts/prepare-db.js",
    "prepare-static-db": "node scripts/prepare-static-db.js",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "docs": "typedoc --entryPointStrategy expand ./src",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy:gh-pages": "node scripts/deploy-gh-pages.js",
    "clean": "rm -rf dist node_modules/.cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bob-takuya/archi-site.git"
  },
  "keywords": [
    "architecture",
    "map",
    "database",
    "japan"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bob-takuya/archi-site/issues"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.8",
    "@mui/material": "^6.4.8",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.4.0",
    "sql.js": "^1.13.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.3",
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.16",
    "@types/node": "^22.13.11",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "kill-port": "^2.0.1",
    "path-browserify": "^1.0.1",
    "sql.js-httpvfs": "^0.8.12",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "typedoc": "^0.25.12",
    "typescript": "^5.8.2",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.3.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^@/(.*)$": "<rootDir>/src/$1",
      "^../../server/utils/queryOptimizer$": "<rootDir>/tests/mocks/queryOptimizer.js",
      "^../server/utils/queryOptimizer$": "<rootDir>/tests/mocks/queryOptimizer.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/tests/pages.test.ts",
      "/tests/basic.test.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}
